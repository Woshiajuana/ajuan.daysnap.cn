{
  "name": "blog.daysnap.cn",
  "private": true,
  "description": "博客网站",
  "scripts": {
    "dev": "concurrently --kill-others \"npm:dev:*\"",
    "dev:link": "ts-node ./scripts/link.ts",
    "dev:source": "pnpm -C ./source dev",
    "dev:website": "pnpm -C ./website dev",
    "build": "npm run build:source && npm run build:website",
    "build:source": "pnpm -C ./source build",
    "build:website": "pnpm -C ./website build",
    "clean": "rimraf **/node_modules"
  },
  "keywords": [
    "daysnap",
    "blog",
    "ajuan",
    "woshiajuana"
  ],
  "author": "Woshiajuana",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "18.15.12",
    "concurrently": "^8.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
